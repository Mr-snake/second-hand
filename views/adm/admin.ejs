		  </ul>

		  <!-- Tab panes -->
		  <section class="tab-content col-lg-10 col-md-10 col-sm-12  col-xs-12">
		  	<!-- 首页-->
		    <div role="tabpanel" class="tab-pane active" id="home">
		    	<form class="form-horizontal" method="post" action="/adm/login/scbannerImg" enctype="multipart/form-data">
		    	  <p class="col-sm-10">我的表单</p>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">标题</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control title" id="inputEmail3" placeholder="标题" name="title">
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputPassword3" class="col-sm-2 control-label">描述</label>
				    <div class="col-sm-10">
				     <textarea class="form-control des" rows="5" name="des"></textarea>
				    </div>
				  </div>
				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="file" class="form-control" id="inputEmail3" placeholder="文件" name="text">
				    </div>
				  </div>
				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="submit">
				    </div>
				  </div>
				</form>
				<table class="table">
					<thead>
						<th>ID</th>
						<th>标题</th>
						<th>描述</th>
	                    <th>图片</th>
	                    <th>操作</th>
					</thead>
					<tbody>
						 <%
                            for(var i=0;i<data.length;i++){
                            %>
                            <tr>
                                <td>
                                    <%=data[i].ID%>
                                </td>
                                <td>
                                    <%=data[i].title%>
                                </td>
                                <td>
                                    <%=data[i].description%>
                                </td>
                                <td>
                                	<img src="../public/upload/<%=data[i].src%>">
                                </td>
                                <td>
                                    <a href="?act=del&id=<%=data[i].ID%>" onclick="return confirm('are you delete?')">删除</a>
                                </td>
                            </tr>
                            <%
                            }
                            %>
					</tbody>
				</table>
		    </div>
		    <!-- 书城小驿 -->
		    <div role="tabpanel" class="tab-pane" id="messages">
		    	<div role="tabpanel" class="tab-pane active" id="home">
		    	<form class="form-horizontal" method="post" action="/adm/login/books" enctype="multipart/form-data">
		    	  <p class="col-sm-10">我的表单</p>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">标题</label>
