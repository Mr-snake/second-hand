	<!-- 标签页 -->
	<section class="container" id="pre">
		  <!-- Nav tabs -->
		  <ul class="nav nav-tabs col-lg-2 col-sm-2 col-md-2 col-xs-12" role="tablist">
		  	<li role="presentation">
		    	<a href="#home"  role="tab" data-toggle="tab" id="hsdlogo">
		    		<img src="../public/images/hsd.jpg" alt="">
		    	</a>
		    </li>
		    <li role="presentation" class="active">
		    	<a href="#home"  role="tab" data-toggle="tab"><i class="glyphicon glyphicon-home"></i>首页</a>
		    </li>
		    <!-- <li role="presentation"><a href="#profile" role="tab" data-toggle="tab"><i class="glyphicon glyphicon-heart"></i></a></li> -->
