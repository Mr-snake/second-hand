				    <label for="inputEmail3" class="col-sm-2 control-label">原价</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control title" id="inputEmail3" placeholder="原价" name="oprice">
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">类型</label>
				    <span class="radio">
					  <label>
					    <input type="radio" name="optionsRadios" id="optionsRadios1" value="food" checked>food
					  </label>
					</span>
					<span class="radio">
					  <label>
					    <input type="radio" name="optionsRadios" id="optionsRadios2" value="foodlist">foodlist
					  </label>
					</span>
				  </div>
				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="file" class="form-control" id="inputEmail3" placeholder="文件" name="text">
				    </div>
				  </div>
				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="submit">
				    </div>
				  </div>
				</form>
		      </div>
		    </div>
		  </div>
