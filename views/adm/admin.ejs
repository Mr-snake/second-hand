				      <input type="text" class="form-control title" id="inputEmail3" placeholder="作者" name="author">
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputPassword3" class="col-sm-2 control-label">描述</label>
				    <div class="col-sm-10">
				     <textarea class="form-control des" rows="5" name="des"></textarea>
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">现价</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control title" id="inputEmail3" placeholder="现价" name="shprice">
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">原价</label>
				    <div class="col-sm-10">
				      <input type="text" class="form-control title" id="inputEmail3" placeholder="原价" name="oprice">
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">类型</label>
				    <span class="radio">
					  <label>
					    <input type="radio" name="optionsRadios" id="optionsRadios1" value="男" checked>男
					  </label>
					</span>
					<span class="radio">
					  <label>
					    <input type="radio" name="optionsRadios" id="optionsRadios2" value="女">女
					  </label>
					</span>
				  </div>

				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="file" class="form-control" id="inputEmail3" placeholder="文件" name="text">
				    </div>
				  </div>
				  <div class="form-group">
				    <div class="col-sm-offset-2 col-sm-10">
				      <input type="submit">
				    </div>
				  </div>
				</form>
		      </div>
		    </div>
		    <!-- 良品铺子 -->
		    <div role="tabpanel" class="tab-pane" id="settings">...</div>
		    <!-- 解忧杂货 -->
		    <div role="tabpanel" class="tab-pane" id="zahuo">
		    	<div role="tabpanel" class="tab-pane active" id="home">
		    	<form class="form-horizontal" method="post" action="/adm/login/common" enctype="multipart/form-data">
		    	  <p class="col-sm-10">解忧杂货</p>
				  
				  <div class="form-group">
				    <label for="inputPassword3" class="col-sm-2 control-label">
				    	产品详情
				    </label>
				    <div class="col-sm-10">
				     <textarea class="form-control des" rows="5" name="des"></textarea>
				    </div>
				  </div>
				  <div class="form-group">
				    <label for="inputEmail3" class="col-sm-2 control-label">现价</label>
